<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <SourceDir>$(SolutionDir)..\src\</SourceDir>
    <XULRunnerDir>$(PROJ_DIR)\xulrunner\xulrunner-36.0.1-obj\dist\</XULRunnerDir>
    <XULDeployDir>$(SolutionDir)xul_$(Configuration)\</XULDeployDir>

    <ExecutablePath>$(PROJ_DIR)\bin;$(XULRunnerDir)\bin;$(ExecutablePath)</ExecutablePath>
    <IncludePath>$(PROJ_DIR)\include;$(PROJ_DIR)\CGAL-4.5.2\include;$(PROJ_DIR)\CGAL-4.5.2\auxiliary\gmp\include;$(PROJ_DIR)\boost\boost_1_57_0;$(IncludePath)</IncludePath>
    <LibraryPath>$(PROJ_DIR)\lib;$(PROJ_DIR)\CGAL-4.5.2\lib;$(PROJ_DIR)\CGAL-4.5.2\auxiliary\gmp\lib;$(PROJ_DIR)\boost\boost_1_57_0\lib32-msvc-12.0;$(LibraryPath)</LibraryPath>

  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>12.0.21005.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(SourceDir);$(SourceDir)\modules;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>_CRT_SECURE_NO_WARNINGS;_SCL_SECURE_NO_WARNINGS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <PrecompiledHeader>Use</PrecompiledHeader>
      <PrecompiledHeaderFile>common.h</PrecompiledHeaderFile>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="SourceDir">
      <Value>$(SourceDir)</Value>
    </BuildMacro>
    <BuildMacro Include="XULRunnerDir">
      <Value>$(XULRunnerDir)</Value>
    </BuildMacro>
    <BuildMacro Include="XULDeployDir">
      <Value>$(XULDeployDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>